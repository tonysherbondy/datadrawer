{{#if isLoop}}
  Loop over table
{{else if isDraw}}
  Draw {{mark}} with
  {{#each displayableAttrs as |attr|}}
    {{instruction-attr-item instruction=instruction attr=attr}}
  {{/each}}
{{else}}
  Adjust
  {{#each displayableAttrs as |attr|}}
    {{instruction-attr-item instruction=instruction attr=attr}}
  {{/each}}
{{/if}}
